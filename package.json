{
  "name": "next-app",
  "version": "0.1.0",
  "private": true,
  "author": "Jacopo Marrone",
  "license": "MIT",
  "scripts": {
    "dev:openbrowser": "npm run open-browser && npm run dev",
    "dev": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "rm -rf .next && next build",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write .",
    "prepare": "husky install",
    "gen:theme-typings": "chakra-cli tokens ./uiLayer.chakra/uiLayer.chakra.customTheme.ts",
    "postinstall": "npm run gen:theme-typings",
    "open-browser": "open http://localhost:3000 && exit 0"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.8",
    "@chakra-ui/react": "^2.2.8",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "framer-motion": "^7.0.1",
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.1.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/node": "18.6.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "cross-env": "^7.0.3",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "typescript": "4.7.4"
  }
}